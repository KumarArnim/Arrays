#include<iostream>
using namespace std;
int RevAlter(int array[] , int num){
    int start = 0;
    int end = num - 1;
    while(start <= end){
        swap(array[start], array[end]);
        start++;
        end--;
    }
}
void PrintArray(int array[] , int num){
    for(int i = 0; i < num; i++){
        cout << array[i] << " ";
    }
}
int main(){
    int arr[100];
    int n;
    cout << "Enter the number of variables you waant to assign in array" << endl;
    cin >> n;
    for (int i = 0; i < n; i++){
        cout << "Enter value at index " << i << " ";
        cin >> arr[i];
    }
    RevAlter(arr, n);
    PrintArray(arr, n);
    return 0;
}
